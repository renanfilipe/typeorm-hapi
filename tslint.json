{
  "extends": ["tslint:all"],
  "linterOptions": {
    "exclude": ["**/*.json"]
  },
  "rules": {
    "no-console": false,
    "completed-docs": [
      true,
      "classes",
      "functions",
      "methods",
      "namespaces",
      "variables"
    ],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "interface-name": [true, "never-prefix"]
  }
}
